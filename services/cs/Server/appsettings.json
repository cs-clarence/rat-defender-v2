{
    "Urls": "http://*:5177",
    "ConnectionStrings": {
        "RatDefender": "Data Source=rat-defender.db.sqlite;Pooling=False"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Slices": {
        "RatDefender": {
            "Domain": {
                "Services": {
                    "RatDetector": {
                        "MinimumConfidence": 0.5,
                        "MinimumTemperatureCelsius": 30,
                        "MaximumTemperatureCelsius": 40
                    }
                }
            }
        },
        "Infrastructure": {
            "Iot": {
                "ThermalImager": {
                    "BusId": 1
                }
            }
        }
    },
    "Mocks": {
        "ThermalImager": true,
        "FoodDispenser": true,
        "Buzzer": true,
        "RatDetectionImageProcessor": true
    }
}
