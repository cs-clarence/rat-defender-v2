<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <RootNamespace>RatDefender.Infrastructure.ObjectDetection.runtime.linux_x64</RootNamespace>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RuntimeIdentifiers>linux-x64</RuntimeIdentifiers>
    </PropertyGroup>

    <ItemGroup>
        <None Include="../../rs/rat-object-detection/build/x86_64-unknown-linux-gnu/release/*"
              CopyToOutputDirectory="PreserveNewest"
              PackagePath="runtimes/linux-x64/native/"
              Pack="true"
        />
    </ItemGroup>
</Project>
